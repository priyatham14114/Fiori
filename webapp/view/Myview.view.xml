<mvc:View
    controllerName="appui5.controller.Myview"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
>   

    <l:VerticalLayout id="idVerticalLayout" binding="{/Actors/1}">
        <Input value="{fName}" editable="false" />
        <Input value="{age}" editable="false"/>
        <Input value="{Born At}" editable="false"/>
        <Input value="{DOB}" editable="false"/>
        <Image src="{photo}" />
        <Button text="Submit" press="onSubmitData"/>
    </l:VerticalLayout>
</mvc:View>
